{
  "expo": {
    "name": "Mindwatch",
    "slug": "Mindwatch",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/mw-logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/mw-logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.maheeenasad.Mindwatch"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/mw-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c8ac6936-3e80-4a9e-8a28-55eea06fea84"
      }
    },
    "owner": "maheeenasad",
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/c8ac6936-3e80-4a9e-8a28-55eea06fea84"
    }
  }
}
